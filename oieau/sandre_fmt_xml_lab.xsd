					<xsd:documentation source="Libelle">
   Type de demande</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ContexteCodification" type="sa_lab:ContexteCodification">
				<xsd:annotation>
					<xsd:documentation source="Libelle">
   Contexte de codification</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="DateDemande" type="sa_lab:DateDemande" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation source="Libelle">
   Date de la demande</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="LbDemande" type="sa_lab:LbDemande" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation source="Libelle">
   Libellé de la demande</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="DateDebutApplicationDemande" type="sa_lab:DateDebutApplicationDemande" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation source="Libelle">
   Date de début d'application de la demande</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="DateFinApplicationDemande" type="sa_lab:DateFinApplicationDemande" minOccurs="0">
