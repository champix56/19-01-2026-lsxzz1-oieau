			</xsd:element>
			<xsd:element name="CommentairesCommanditaire" type="sa_lab:CommentairesCommanditaire" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation source="Libelle">
   Commentaires sur la demande</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Payeur" type="sa_int:Intervenant" minOccurs="0">
